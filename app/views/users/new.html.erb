<h1> Create an Account </h1>
<%= form_with(model: @user, url:'/signup', local: true) do |f|%>
    <%= f.label :name %>:
    <%= f.text_field :name %><br><br>
    <%= f.label :email %>:
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>:
    <%= f.password_field :password %>
    <%= f.submit "Sign Up"%>
<% end %>